

//come.opentutorials.web - 클래스 name
//Nana.java 

import.java.io.IOException;
import.hava.io.PrinterWiter;

import.javax.servlet.SerbletRequest;
import .javax.servlet.ServletRequest;
import .javax.servlet.ServletRequest;

@WebServlet("/Nana")    //서블렛 주석

public class Nana extends HttpServlet{                        //HttpServlet을 상속받아야 하며, protected void service(HttpServletRequest request, HttpServletReponse) 메소드를
                                                              //호출하여 overriding 해주어야 한다.

@overriding 

 protected void service(HttpServletRequest request, HttpServletReponse) throws IERception {
 
    response.setCharacterEncoding("UTF-8");                      //Encoding 인코딩 방식
    response.setContentType("text/html; charset-UTF-8");         //한글 출력 지원

    PrinterWriter out=response.getWirter();
    
    out.printlm("안녕하세요!!");
 } 
}


